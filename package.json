{
	"name": "react-bits",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "npm run build:registry && vite build",
		"build:registry": "concurrently -p \"[{name}]\" -n \"default,tailwind,ts-default,ts-tailwind,readmes\" \"npm run build:registry:default\" \"npm run build:registry:tailwind\" \"npm run build:registry:ts-default\" \"npm run build:registry:ts-tailwind\" \"npm run build:registry:readmes\"",
		"build:registry:default": "jsrepo build --dirs ./src/content --output-dir ./public/default",
		"build:registry:tailwind": "jsrepo build --dirs ./src/tailwind --output-dir ./public/tailwind",
		"build:registry:ts-default": "jsrepo build --dirs ./src/ts-default --output-dir ./public/ts/default",
		"build:registry:ts-tailwind": "jsrepo build --dirs ./src/ts-tailwind --output-dir ./public/ts/tailwind",
		"build:registry:readmes": "jsrepo build --dirs ./src/readmes --output-dir ./public/readmes",
		"new:component": "node scripts/generateComponent.js",
		"lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview"
	},
	"dependencies": {
		"@chakra-ui/icons": "^2.2.0",
		"@chakra-ui/react": "^2.9.0",
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.0",
		"@react-spring/web": "^9.8.0",
		"@react-three/drei": "^9.122.0",
		"@react-three/fiber": "^8.18.0",
		"@react-three/postprocessing": "^2.20.0",
		"@react-three/rapier": "^1.5.0",
		"@tailwindcss/vite": "^4.1.0",
		"framer-motion": "^11.4.0",
		"gl-matrix": "^3.5.0",
		"gsap": "^3.13.0",
		"matter-js": "^0.21.0",
		"meshline": "^3.4.0",
		"ogl": "^1.1.0",
		"postprocessing": "^6.37.0",
		"react": "^18.4.0",
		"react-confetti": "^6.3.0",
		"react-dom": "^18.4.0",
		"react-haiku": "^2.3.0",
		"react-helmet-async": "^2.1.0",
		"react-icons": "^5.3.0",
		"react-router-dom": "^6.27.0",
		"react-syntax-highlighter": "^15.6.0",
		"react-use-gesture": "^9.2.0",
		"sonner": "^1.8.0",
		"tailwindcss": "^4.1.0",
		"three": "^0.168.0"
	},
	"devDependencies": {
		"@types/matter-js": "^0.20.0",
		"@types/react": "^18.4.0",
		"@types/react-dom": "^18.4.0",
		"@vitejs/plugin-react": "^4.4.0",
		"concurrently": "^10.0.0",
		"eslint": "^8.60.0",
		"eslint-plugin-react": "^7.35.0",
		"eslint-plugin-react-hooks": "^4.7.0",
		"eslint-plugin-react-refresh": "^0.5.0",
		"jsrepo": "^1.31.0",
		"postcss-safe-parser": "^7.1.0",
		"typescript": "^5.8.0",
		"vite": "^5.4.0"
	}
}
